package org.example;

import java.util.Scanner;
/*
1-100 kadar olan asal sayÄ±lar.
 */
public class Main {
    public static void main(String[] args) {
        Scanner scanner=new Scanner(System.in);
        for (int i = 2; i<=100 ; i++) {
            if(i%2!=0 && i%3!=0 && i%5!=0 && i%7!=0 ){
                System.out.print("-"+i+"-");
            }
            else if(i==2 || i==3 || i==5 || i==7)
                System.out.print("-"+i+"-");


        }

    }
}
